describe('moving forward by leaving privacy box unchecked', () => {
    it('log in test', () => {
        cy.visit('https://storymaps.arcgis.com/')
        cy.wait(700)
        cy.get('.jsx-4157471165').click()
        cy.wait(700)
        cy.get('#createAcctLink').click()
        cy.wait(700)
        cy.get('#firstName').type('name')
        cy.wait(700)
        cy.get('#lastName').type('lastname')
        cy.wait(700)
        cy.get('#email').type('email')
        cy.wait(700)
        cy.get('#confirm').type('email')
        cy.wait(700)
        cy.get('#terms').check()
        cy.wait(700)
        cy.get('#privacy').check()
        cy.wait(700)
        cy.get('#terms').uncheck()
        cy.wait(800)
        cy.get('#nextBtn').click()
    })
});
